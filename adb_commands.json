{
  "1 Navigate To Home Screen": "adb -s {SERIAL} shell am start -a android.intent.action.MAIN -c android.intent.category.HOME",
  "2 Application Install": "adb -s {SERIAL} install {APK}",
  "3 Application Uninstall": "adb -s {SERIAL} uninstall {PACKAGE}",
  "4 To view apk properties": "aapt dump badging {PATH_TO_APK}",
  "5 To Remove the Default Launcher": "adb -s {SERIAL} shell rm /system/app/Launcher3.apk",
  "6 Android Running Services": "adb -s {SERIAL} shell dumpsys activity services",
  "7 Android Version": "adb -s {SERIAL} shell getprop ro.build.version.release",
  "8 Android SDK Version": "adb -s {SERIAL} shell getprop ro.build.version.sdk",
  "9 Android properties": "adb -s {SERIAL} shell getprop",
  "10 Date & Time": "adb -s {SERIAL} shell date",
  "11 Application Force-Stop": "adb -s {SERIAL} shell am force-stop {PACKAGE}",
  "12 Running Processes": "adb -s {SERIAL} shell ps",
  "13 adb shell": "adb -s {SERIAL} shell",
  "14 Network status": "adb -s {SERIAL} shell netcfg",
  "15 To get a file": "adb -s {SERIAL} pull path_to_file",
  "16 To put a file": "adb -s {SERIAL} push sourcePath destinationPath",
  "17 To Change time Zone": "adb -s {SERIAL} shell setprop persist.sys.timezone {TIMEZONE}",
  "18 To take backups": "adb -s {SERIAL} backup -apk -shared -all",
  "19 To Restore": "adb -s {SERIAL} restore backup.ab",
  "20 To save logs from android": "adb -s {SERIAL} shell logcat > log.txt",
  "21 To check CPU usage": "adb -s {SERIAL} shell dumpsys cpuinfo",
  "22 To CheckMemory usage": "adb -s {SERIAL} shell dumpsys meminfo",
  "23 To Change Settings": "adb -s {SERIAL} shell am start -n com.android.settings/.Settings",
  "24 Bug Report Via adb -s {SERIAL}": "adb -s {SERIAL} bugreport > bug_report.txt",
  "25 Bug Report Parsing": "java -jar chkbugreport.jar bug_report.txt",
  "26 To Reboot the OS": "adb -s {SERIAL} shell reboot",
  "27 adb Version": "adb version",
  "28 To get Help": "adb help",
  "29 To get Serial Number": "adb -s {SERIAL} get-serialno",
  "30 List of devices": "adb -s {SERIAL} devices",
  "31 To get running process": "adb -s {SERIAL} shell ps",
  "32 To print dump": "adb -s {SERIAL} shell dumpsys",
  "33 To start an Activity": "adb -s {SERIAL} shell am start PACKAGE_NAME/.ACTIVITY_NAME",
  "34 To save screenshot": "adb -s {SERIAL} shell screencap -p /mnt/image_name.png",
  "35 List of installed packages": "adb -s {SERIAL} shell pm list packages -f",
  "36 To make key event": "adb -s {SERIAL} shell input keyevent"
}