{
  "1 Navigate To Home Screen": "{ADB_COMMAND} -s {SERIAL} shell am start -a android.intent.action.MAIN -c android.intent.category.HOME",
  "2 Application Install": "{ADB_COMMAND} -s {SERIAL} install {APK}",
  "3 Application Uninstall": "{ADB_COMMAND} -s {SERIAL} uninstall {PACKAGE}",
  "4 To view apk properties": "aapt dump badging {PATH_TO_APK}",
  "5 To Remove the Default Launcher": "",
  "6 Android Running Services": "{ADB_COMMAND} -s {SERIAL} shell dumpsys activity services",
  "7 Android Version": "{ADB_COMMAND} -s {SERIAL} shell getprop ro.build.version.release",
  "8 Android SDK Version": "{ADB_COMMAND} -s {SERIAL} shell getprop ro.build.version.sdk",
  "9 Android properties": "{ADB_COMMAND} -s {SERIAL} shell getprop",
  "10 Date & Time": "{ADB_COMMAND} -s {SERIAL} shell date",
  "11 Application Force-Stop": "{ADB_COMMAND} -s {SERIAL} shell am force-stop {PACKAGE}",
  "12 Running Processes": "{ADB_COMMAND} -s {SERIAL} shell ps",
  "13 ADB shell": "{ADB_COMMAND} -s {SERIAL} shell",
  "14 Network status": "{ADB_COMMAND} -s {SERIAL} shell netcfg",
  "15 To get a file": "{ADB_COMMAND} -s {SERIAL} pull path_to_file",
  "16 To put a file": "{ADB_COMMAND} -s {SERIAL} push sourcePath destinationPath",
  "17 To Change time Zone": "{ADB_COMMAND} -s {SERIAL} shell setprop persist.sys.timezone {TIMEZONE}",
  "18 To take backups": "{ADB_COMMAND} -s {SERIAL} backup -apk -shared -all",
  "19 To Restore": "{ADB_COMMAND} -s {SERIAL} restore backup.ab",
  "20 To save logs from android": "{ADB_COMMAND} -s {SERIAL} shell logcat > log.txt",
  "21 To check CPU usage": "{ADB_COMMAND} -s {SERIAL} shell dumpsys cpuinfo",
  "22 To CheckMemory usage": "{ADB_COMMAND} -s {SERIAL} shell dumpsys meminfo",
  "23 To Change Settings": "{ADB_COMMAND} -s {SERIAL} shell am start -n com.android.settings/.Settings",
  "24 Bug Report Via adb -s {SERIAL}": "{ADB_COMMAND} -s {SERIAL} bugreport > bug_report.txt",
  "25 Bug Report Parsing": "java -jar chkbugreport.jar bug_report.txt",
  "26 To Reboot the OS": "{ADB_COMMAND} -s {SERIAL} shell reboot",
  "27 adb Version": "{ADB_COMMAND} version",
  "28 To get Help": "{ADB_COMMAND} help",
  "29 To get Serial Number": "{ADB_COMMAND} -s {SERIAL} get-serialno",
  "30 List of devices": "{ADB_COMMAND} -s {SERIAL} devices",
  "31 To get running process": "{ADB_COMMAND} -s {SERIAL} shell ps",
  "32 To print dump": "{ADB_COMMAND} -s {SERIAL} shell dumpsys",
  "33 To start an Activity": "{ADB_COMMAND} -s {SERIAL} shell am start PACKAGE_NAME/.ACTIVITY_NAME",
  "34 To save screenshot": "{ADB_COMMAND} -s {SERIAL} shell screencap -p /mnt/image_name.png",
  "35 List of installed packages": "{ADB_COMMAND} -s {SERIAL} shell pm list packages -f",
  "36 To make key event": "{ADB_COMMAND} -s {SERIAL} shell input keyevent"
}